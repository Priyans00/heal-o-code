{
    "version": 2,
    "builds": [
        {
            "src": "api/supa.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/supa.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "VERCEL_ENV": "production"
    }
}